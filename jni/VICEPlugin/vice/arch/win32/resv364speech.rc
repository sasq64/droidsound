// V364 Speech specific resources.
//

// template of dialog for all languages

IDD_V364SPEECH_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX  "", IDC_V364SPEECH_ENABLE,
                  7, 7, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT         "", IDC_V364SPEECH_FILE_LABEL,
                  9, 24, 30, 8
    PUSHBUTTON    "", IDC_V364SPEECH_BROWSE,
                  45, 22, 50, 14, 0
    EDITTEXT      IDC_V364SPEECH_FILE,
                  9, 38, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 90, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 90, 50, 14, WS_TABSTOP
END

