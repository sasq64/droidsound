// template of dialog for all languages

IDD_DQBB_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Double Quick Brown Box settings"
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX    "",IDC_DQBB_ENABLE,
                    7, 7, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "",IDC_DQBB_WRITE_ENABLE,
                    7, 32, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT           "", IDC_DQBB_FILE_LABEL,
                    9, 44, 46, 8
    PUSHBUTTON      "", IDC_DQBB_BROWSE,
                    55, 42, 50, 14, 0
    EDITTEXT        IDC_DQBB_FILE,
                    9, 58, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    30, 90, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    90, 90, 50, 14, WS_TABSTOP
END

